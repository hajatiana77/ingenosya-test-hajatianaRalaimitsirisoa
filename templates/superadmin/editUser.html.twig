{% extends 'superadmin/base_superadmin.html.twig' %}

{% block title %} {{'Update form'|trans}} {% endblock %}

{% block content %}
    <style type="text/css">
        .center{
            margin-left: 5%; width:50%;
        }
    </style>    
    <div class="center">
        <h1>{{'Update form'|trans}} {{ username }}</h1>
        
        {{ form_start(form) }}
        {{ form_widget(form) }}    
        <input type="submit" value="{{ 'Save'|trans }}" />
        {{ form_end(form) }}
    </div>
{% endblock %}